<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ar.edu.utn.frsf.isi.ia.PatrulleroUI.gui.controladores.VerObstaculosLugarController">
   <children>
      <TableView fx:id="tablaObstaculos">
         <columns>
            <TableColumn fx:id="columnaId" minWidth="-1.0" prefWidth="100.0" text="Id" />
            <TableColumn fx:id="columnaTipo" minWidth="-1.0" prefWidth="100.0" text="Tipo" />
            <TableColumn fx:id="columnaNombre" minWidth="-1.0" prefWidth="100.0" text="Nombre" />
            <TableColumn fx:id="columnaTiempoInicio" minWidth="-1.0" prefWidth="100.0" text="Tiempo Inicio" />
            <TableColumn fx:id="columnaTiempoFin" minWidth="-1.0" prefWidth="100.0" text="Tiempo Fin" />
            <TableColumn fx:id="columnaVisibilidad" minWidth="-1.0" prefWidth="100.0" text="Visibilidad" />
            <TableColumn fx:id="columnaRetardo" minWidth="-1.0" prefWidth="100.0" text="Retardo" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
